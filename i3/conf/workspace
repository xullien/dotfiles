# move app to workspace
for_window [class="discord"] move container to workspace 2
for_window [class="Moonlight"] move container to workspace 3
for_window [class="com.moonlight_stream.Moonlight"] move container to workspace 3

# floating windows
# use xprop to see the class and instance of the software you want to fload
# for_window [class="TelegramDesktop" instance="telegram-desktop"] floating enable
for_window [class="TelegramDesktop" instance="telegram-desktop"] floating enable, move position center
for_window [class="Pulseaudio-equalizer-gtk" instance="pulseaudio-equalizer-gtk"] floating enable, move position center
for_window [class="Plex" instance="plex-bin"] floating enable, move position center
for_window [class="Plexamp" instance="plexamp"] floating enable, move position 1295 760 
for_window [class="xpad" instance="xpad"] floating enable  
for_window [class="Nitrogen" instance="nitrogen"] floating enable, move position center  
for_window [class="Blueman-manager" instance="blueman-manager"] floating enable, move position center  
for_window [class="Lua5.1" instance="lua5.1"] floating enable, move position center  
for_window [class="Skype" instance="skype"] floating enable
for_window [class="kclock" instance="kclock"] floating enable, move position center  
